@model IEnumerable<AssetManager.Core.Entities.Asset>

@{
    ViewData["Title"] = "Index";
}

<h2>All Assets</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<div class="row">
    <div class="col-md-12">

        <div class="box box-default">
            <div class="box-body">
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                
                                <th>
                                    @Html.DisplayNameFor(model => model.AssetTag)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ModelsId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.StatusId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Serial)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Name)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.PurchaseDate)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.SupplierId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.OrderNumber)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.PurchaseCost)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.WarrantyMonths)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Notes)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.LocationId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Requestable)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Image)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.CheckoutTo)
                                </th>
                               
                                <th>
                                    @Html.DisplayNameFor(model => model.Physical)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.DeletedAt)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Archived)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Depreciate)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Accepted)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.LastCheckout)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ExpectedCheckin)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.AssignedType)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.LastAuditDate)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.NextAuditDate)
                                </th>

                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    
                                    <td>
                                        @Html.DisplayFor(modelItem => item.AssetTag)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ModelsId)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.StatusId)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Serial)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PurchaseDate)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.SupplierId)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.OrderNumber)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PurchaseCost)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.WarrantyMonths)
                                    </td>
