                                        @Html.DisplayFor(modelItem => item.Notes)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.LocationId)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Requestable)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Image)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.CheckoutTo)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Physical)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.DeletedAt)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Archived)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Depreciate)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Accepted)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.LastCheckout)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ExpectedCheckin)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.AssignedType)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.LastAuditDate)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.NextAuditDate)
                                    </td>

                                    <td>
                                        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                                        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                                        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
 </div>

